<?xml version="1.0" encoding="UTF-8" ?>
<GpuProgram>
	<vertexShader ref="shaders/Shader2D.vp" />
	<fragmentShader ref="shaders/Shader2D.fp" />
	
	<attribute>
		<name>vertex</name>
		<type>VERTEX</type>
	</attribute>
	<attribute>
		<name>texcoord0</name>
		<type>TEX_COORD_0</type>
